server {
    listen 443 ssl;
    server_name n73.agustfricke.com www.n73.agustfricke.com;

    ssl_certificate /etc/letsencrypt/live/n73.agustfricke.com/fullchain.pem; 
    ssl_certificate_key /etc/letsencrypt/live/n73.agustfricke.com/privkey.pem; 

    location / {
        proxy_pass http://127.0.0.1:80;
        proxy_http_version 1.1;
        proxy_set_header Upgrade $http_upgrade;
        proxy_set_header Connection "upgrade";
        proxy_set_header Host $host;
    }
}
