server {
    listen 443 ssl;
    server_name www.ai.zustack.com ai.zustack.com;

    ssl_certificate /etc/letsencrypt/live/ai.zustack.com/fullchain.pem; 
    ssl_certificate_key /etc/letsencrypt/live/ai.zustack.com/privkey.pem; 

    location / {
        proxy_pass http://127.0.0.1:80;
        proxy_http_version 1.1;
        proxy_set_header Upgrade $http_upgrade;
        proxy_set_header Connection "upgrade";
        proxy_set_header Host $host;
    }
}
